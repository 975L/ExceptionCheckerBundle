    "description": "Listener for Symfony Exceptions in order to redirect to another url, or throw GoneException with a list of urls",
    "keywords": ["symfony", "bundle", "exception", "redirect"],
    "homepage": "https://github.com/975L/ExceptionCheckerBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Laurent Marquet",
            "email": "laurent.marquet@laposte.net",
            "homepage": "https://github.com/Laurent3170",
            "role": "Developer"
        },
        {
            "name": "975L Community",
            "homepage": "https://github.com/975L"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "doctrine/doctrine-bundle": "^1",
        "doctrine/orm": "^2",
        "knplabs/knp-paginator-bundle": "^2",
        "c975l/toolbar-bundle": "^1",
        "c975l/includelibrary-bundle": "^1",
        "c975l/config-bundle": "^2",
        "c975l/services-bundle": "^1"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": { "c975L\\ExceptionCheckerBundle\\": ""
        }
    },
    "support": {
        "email": "contact@975l.com",
        "source": "https://github.com/975L/ExceptionCheckerBundle",
        "issues": "https://github.com/975L/ExceptionCheckerBundle/issues"
    }
